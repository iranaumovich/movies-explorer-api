{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "root": true,
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
